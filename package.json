    "express": "^4.16.4",
    "express-handlebars": "^3.0.2",
    "express-validator": "^5.3.1",
    "firebase-admin": "^7.3.0",
    "handlebars": "^4.1.2",
    "hbs": "^4.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nodemailer": "^6.1.1",
    "nodemailer-express-handlebars": "^3.0.0",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "path": "^0.12.7",
